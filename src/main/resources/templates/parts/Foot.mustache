        $( '.navbar' ).find( 'li.active' ).removeClass( 'active' );
        $( this ).parent( 'li' ).addClass( 'active' );

    });
        var activeTab = localStorage.getItem('activeTab');
        if(activeTab){
            $('.navbar a[href="' + activeTab + '"]').addClass( 'active' );
        }
});
</script>
